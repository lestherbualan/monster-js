  },
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "jasmine": "^4.4.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.3"
